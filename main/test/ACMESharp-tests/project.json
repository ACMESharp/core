{
  "version": "1.0.0-*",
  "buildOptions": {
    "debugType": "portable",
    "embed": {
      "mappings": {
        "ACMESharp/": [
          "**/input.txt",
          "**/*.cut",
          "**/*.str"
        ]
      }
    },
    // As instructed here:
    //    http://xunit.github.io/docs/configuring-with-json.html#dotnet-cli
    "copyToOutput": {
      "include": [
        "xunit.runner.json"
      ]
    }
  },

  "dependencies": {
    "System.Runtime.Serialization.Primitives": "4.1.1",
    "xunit": "2.1.0",
    "dotnet-test-xunit": "1.0.0-rc2-192208-24",
    "ACMESharp": {
      "target": "project"
    }
  },
  "testRunner": "xunit",
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.0.0"
        }
      },
      "imports": [
        "dotnet5.4",
        "portable-net451+win8"
      ]
    }
  }
}
